{
	"name": "SteamAuth2",
	"version": "2.0",
	"author": [
		"[https://www.mediawiki.org/wiki/User:Banakin900 Banakin]",
		"[https://github.com/clamburger Clamburger]"
	],
	"license-name": "MIT",
	"url": "https://github.com/clamburger/MediaWiki-SteamAuth2",
	"descriptionmsg": "steamauth2-desc",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.34.0",
		"extensions": {
			"PluggableAuth": ">= 6.0"
		}
	},
	"MessagesDirs": {
		"SteamAuth2": [
			"i18n"
		]
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\SteamAuth2\\": "src/"
	},
	"AutoloadClasses": {
		"LightOpenID": "includes/openid.php"
	},
	"config": {
		"SteamAuth2_Key": {
			"value": "key",
			"description": "steamauth2-key-dec"
		},
		"SteamAuth2_AppID": {
			"value": null,
			"description": "steamauth2-appid-dec"
		}
	},
	"ConfigRegistry": {
		"SteamAuth2": "GlobalVarConfig::newInstance"
	},
	"attributes": {
		"PluggableAuth": {
			"SteamAuth2": {
				"class": "MediaWiki\\Extension\\SteamAuth2\\SteamAuth",
				"services": [
					"AuthManager"
				]
			}
		}
	},
	"load_composer_autoloader": true,
	"manifest_version": 2
}
